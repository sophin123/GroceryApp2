<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.groceryapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_grocery"
        android:label="GroceryApp"
        android:roundIcon="@mipmap/ic_launcher_grocery"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label">
        <activity android:name=".MainAcitivty.mainpage.mainpage.GroceryActivity.MainActivity" />
        <activity android:name=".MainAcitivty.mainpage.mainpage.GroceryActivity.NoteActivity.EditDataActivity" />
        <activity android:name=".MainAcitivty.mainpage.mainpage.GroceryActivity.NoteActivity.ListDataActivity" />
        <activity android:name=".MainAcitivty.mainpage.mainpage.GroceryActivity.NoteActivity.Main2Activity" />
        <activity
            android:name=".MainAcitivty.mainpage.mainpage.GroceryActivity.splashscreen"
            android:label="MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>